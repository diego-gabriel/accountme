import static org.junit.Assert.*;

import org.junit.Test;


public class ArrayListTest {

	@Test
	public void test() {
		ArrayList<Integer> array = new ArrayList<Integer>();
		
		array.add(1);
		assertEquals(1, array.size());

		array.add(10);
		assertEquals(2, array.size());

		array.add(7);
		assertEquals(3, array.size());
		
		int res = array.get(1);
		assertEquals(10, res);
		
		array.insert(2, 10);
		array.insert(1, 2);
		
		res = array.get(2)
		assertEquals(10, res);
		
	}

}
